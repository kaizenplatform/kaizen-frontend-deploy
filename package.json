{
  "name": "kaizen-frontend-deploy",
  "version": "0.0.2",
  "private": true,
  "description": "CLI tool for deploying SPA to Amazon S3 and CloudFront",
  "files": [
    "dist/"
  ],
  "bin": {
    "kaizen-frontend-deploy": "./dist/index.js"
  },
  "scripts": {
    "build": "babel src -d dist",
    "test": "test/bin/bats test/index.spec.sh",
    "lint": "eslint index.js lib",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "preversion": "npm test",
    "version": "npm run build && npm run changelog && git add -A dist CHANGELOG.md",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "aws-sdk": "^2.111.0",
    "camelcase": "^4.1.0",
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "recursive-readdir": "^2.2.1",
    "s3": "^4.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "conventional-changelog-cli": "^1.3.3",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  }
}
